public class Baralho {
	
	static int qtdeCartas = 52;
	
	List<Carta> cartas;
	
	Baralho (){
		Carta c = new Carta();
		c.valor = "A";
		c.naipe = "h";
		
	}
	
	void embaralhar() {
		
	}
	
	Carta obterCarta () {
		qtdeCartas -= 1;
		return new Carta();
	}

}
